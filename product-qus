import java.util.Scanner;

// product *rem;
public class product_qus {
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        System.out.println("enter the number here ");
        int num = sc.nextInt();
        int product =1;
        while(num>0)
        {
            int rem = num % 10;
            if (rem % 2 == 0)
            {
                product = product * rem;
            }
            num = num / 10;
        }
        System.out.println("product of digits:" + product);
    }
}
